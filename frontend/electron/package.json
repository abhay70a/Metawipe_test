 {
  "name": "metawipe-electron",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"npm run dev --prefix ..\" \"wait-on http://localhost:8080 && electron .\"",


    "build": "npm run build --prefix .. && electron-builder"
  },
  "devDependencies": {
    "electron": "^30.0.0",
    "electron-builder": "^25.0.0",
    "concurrently": "^8.2.0",
    "wait-on": "^7.0.1"
  }
}
